<odoo>
  <data>
    <record model="ir.ui.view" id="cb_account_invoice_supplier_form_inherit">
      <field name="name">account.invoice.supplier.form.inherit</field>
      <field name="model">account.invoice</field>
      <field name="inherit_id" ref="account.invoice_supplier_form"/>
      <field name="arch" type="xml">
        <xpath expr="//form/sheet/notebook/page/field/tree/field[@name='price_subtotal']" position="after">
          <field name="planned" invisible='1'/>
          <field name="available" invisible='1'/>
        </xpath>
      </field>
    </record>
  </data>
</odoo>
